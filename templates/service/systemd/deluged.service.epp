<%- |   String  $service_user,
| -%>
[Unit]
Description=Deluge Bittorrent Client Daemon
Documentation=man:deluged
After=network-online.target

[Service]
Type=simple
User=<%= $service_user %>
Group=<%= $service_user %>
UMask=007

ExecStart=/usr/bin/deluged -d -l /var/log/deluge/daemon.log -L warning

Restart=on-failure

TimeoutStopSec=300

[Install]
WantedBy=multi-user.target
